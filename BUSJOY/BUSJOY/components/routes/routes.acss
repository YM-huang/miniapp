.routes-wraper .top-wraper {
  display: flex;
  justify-content: space-between;
  text-align: left;
  padding: 0 32rpx;
}
.routes-wraper .top-wraper .label {
  font-size: 32rpx;
  line-height: 40rpx;
  vertical-align: middle;
  font-weight: 500;
}
.routes-wraper .top-wraper .more .pre {
  font-size: 24rpx;
  color: #999;
  vertical-align: middle;
}
.routes-wraper .top-wraper .more .arrow {
  width: 12rpx;
  height: 21rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}
.routes-wraper .content-wraper {
  margin-top: 20rpx;
}
.routes-wraper .content-wraper .item {
  display: flex;
  justify-content: space-between;
  padding-left: 32rpx;
  padding-top: 28.5rpx;
  border-bottom: 1rpx solid #eee;
}
.routes-wraper .content-wraper .item:last-child {
  border-bottom: none;
}
.routes-wraper .content-wraper .item .label .route {
  font-size: 26rpx;
  color: #FFF;
  width: 119rpx;
  height: 44rpx;
  font-weight: 500;
  border-radius: 22.5rpx;
  line-height: 44rpx;
}
.routes-wraper .content-wraper .item .label .price {
  font-size: 22rpx;
  color: #999;
  margin-top: 8rpx;
}
.routes-wraper .content-wraper .item .detail {
  width: 505rpx;
  text-align: left;
}
.routes-wraper .content-wraper .item .detail .line {
  padding: 28.5rpx 32rpx 28.5rpx 0;
  border-bottom: 1rpx solid #eee;
}
.routes-wraper .content-wraper .item .detail .line:last-child {
  border-bottom: none;
}
.routes-wraper .content-wraper .item .detail .line:first-child {
  padding-top: 0;
}
.routes-wraper .content-wraper .item .detail .line .info {
  display: flex;
  justify-content: space-between;
  vertical-align: middle;
}
.routes-wraper .content-wraper .item .detail .line .info .content .tartget {
  font-size: 28rpx;
}
.routes-wraper .content-wraper .item .detail .line .info .content .time {
  font-size: 24rpx;
  color: #333;
  margin-top: 6rpx;
}
.routes-wraper .content-wraper .item .detail .line .info .arrow {
  width: 12rpx;
  height: 21rpx;
  margin-top: 28rpx;
  margin-left: 10rpx;
}
.routes-wraper .content-wraper .item .detail .line .gap {
  font-size: 24rpx;
  color: #999999;
  margin-top: 12rpx;
}
