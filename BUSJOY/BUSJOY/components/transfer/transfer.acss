.transfer-wraper {
  text-align: left;
}
.transfer-wraper-title {
  font-size: 36rpx;
  padding: 0 32rpx;
  font-weight: 500;
}
.transfer-wraper-map {
  padding: 0 32rpx;
  margin-top: 26rpx;
  padding-bottom: 32.5rpx;
}
.transfer-wraper-map-title {
  display: flex;
  justify-content: space-between;
  margin-bottom: 19rpx;
}
.transfer-wraper-map-title-count-content {
  font-size: 32rpx;
  color: #108EE9;
  font-weight: bold;
}
.transfer-wraper-map-title-count-label {
  font-size: 26rpx;
}
.transfer-wraper-map-title-discount {
  font-size: 26rpx;
  color: #999999;
}
.transfer-wraper-map-content-map {
  width: 638rpx!important;
  height: 193rpx!important;
}
.transfer-wraper-traffic {
  display: flex;
  padding: 32rpx 32rpx 0;
  justify-content: space-between;
  border-top: 1rpx solid #eee;
  font-size: 26rpx;
  font-weight: normal;
}
.transfer-wraper-traffic .label .pre {
  display: inline-block;
  vertical-align: middle;
}
.transfer-wraper-traffic .label .icon {
  display: inline-block;
  height: 34rpx;
  background: rgba(152, 227, 199, 0.24);
  line-height: 34rpx;
  padding: 0 8rpx;
  font-size: 20rpx;
  color: #11A86E;
  font-weight: 500;
  border-radius: 2rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}
.transfer-wraper-traffic .go .text {
  vertical-align: middle;
}
.transfer-wraper-traffic .go .arrow {
  width: 12rpx;
  height: 21rpx;
  margin-left: 10rpx;
  vertical-align: middle;
}
