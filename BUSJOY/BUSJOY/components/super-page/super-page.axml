<view class="m-super-page">
  <view class="m-super-page-header {{pageHeaderCls}}" style="{{paddingTop: statusAndTitleBarHeight + 'rpx'}}">
    <slot name="{{pageHeaderCls}}"></slot>
  </view>
  <view class="m-super-page-body {{pageBodyCls}}" name="page-body">
    <slot name="{{pageBodyCls}}"></slot>
  </view>
  <view class="m-super-page-foot {{pageFootCls}}" name="page-foot">
    <slot name="page-foot"></slot>
    <view class="m-super-page-bottom-content"> {{bottomContent}} </view>
  </view>
  
</view>