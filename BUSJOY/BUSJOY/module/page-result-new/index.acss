.am-page-result {
  background: #fff;
  overflow: hidden;
}

.am-page-result-pic {
  width: 220px;
  height: 220px;
  margin: 22px auto 0;
  background-position: center bottom;
  background-repeat: no-repeat;
}

.page-network {
  background-image: url("https://gw.alipayobjects.com/zos/rmsportal/EUEXznElQXFzReDEGTNe.png");
  background-size: 140px 150px;
}

.page-error {
  background-image: url("https://gw.alipayobjects.com/zos/rmsportal/gofJqdAUNyIMWsdntOds.png");
  background-size: 158px 70px;
}

.page-busy {
  background-image: url("https://gw.alipayobjects.com/zos/rmsportal/hVLFyrBzVZqsSOoHoJEn.png");
  background-size: 104px 133px;
}

.page-empty {
  background-image: url("https://gw.alipayobjects.com/zos/rmsportal/fRWLsYABGKDPhaogZUiF.png");
  background-size: 146px 146px;
}

.page-logoff {
  background-image: url("https://gw.alipayobjects.com/zos/rmsportal/KVuziFDymkQgDIgRoppG.png");
  background-size: 94px 131px;
}

.am-page-result-title {
  margin: 20px auto 0;
  text-align: center;
  font-size: 20px;
  color: #333;
}

.am-page-result-brief {
  margin: 12px auto;
  font-size: 14px;
  color: #888;
  max-width: 266px;
  text-align: center;
}

.am-page-result-option{
  font-family: PingFangSC-Regular;
  font-size: 20px;
  color: #108EE9;
  position: absolute;
  bottom: 8%;
  width: 100%;
  text-align: center;
}

.am-local-page {
  height: 100%;
}

.am-local-page .am-page-result-pic {
  width: 100px;
  height: 105px;
  margin: 0 auto;
}

.am-local-page  .am-page-result-brief {
  margin: 21px auto 30px;
  font-size: 14px;
  color: #999;
  max-width: 266px;
  text-align: center;
}

.am-local-page .page-empty {
  background-image: url("https://gw.alipayobjects.com/zos/rmsportal/eHKXaGEOdWABrhOSwwWf.png");
  background-size: 45px 77px;
}

.am-local-page .page-error {
  background-image: url("https://gw.alipayobjects.com/zos/rmsportal/FNXCltKToIboiMnxqxzv.png");
  background-size: 88px 42px;
}

.am-local-page .page-network {
  background-image: url("https://gw.alipayobjects.com/zos/rmsportal/aarIJKkaawUZsGHEDsyv.png");
  background-size: 78px 73px;
}

.am-local-page .page-busy {
  background-image: url("https://gw.alipayobjects.com/zos/rmsportal/vuIYcaowFdPnBVbqZHrn.png");
  background-size: 57px 86px;
}

.am-local-page .page-logoff {
  background-image: url("https://gw.alipayobjects.com/zos/rmsportal/ZlXHUijnungMocFNdJwX.png");
  background-size: 59px 81px;
}
