.accumulation-container {
  height: 102rpx;
  background: #fff;
  border-radius: 16rpx;
  padding: 0 24rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.accumulation-container .left-content {
  display: flex;
  align-items: center;
}
.accumulation-container .accumulation-container-line {
  margin: 0 24rpx;
  width: 1rpx;
  height: 40rpx;
  background: #E4E4E4;
}
.accumulation-container .accumulation-container-text {
  font-size: 28rpx;
  color: #333;
}
.accumulation-container .accumulation-container-image {
  width: 54rpx;
  height: 54rpx;
}
.accumulation-container .right-content {
  width: 9rpx;
  height: 14rpx;
}
