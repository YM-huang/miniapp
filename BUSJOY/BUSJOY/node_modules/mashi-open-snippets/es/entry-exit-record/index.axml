<!-- 移民局-出入境记录 -->
<view class="entry-exit-record-wraper {{containerClassName}}">
    <view class="entry-exit-record-top">
        <view class="entry-exit-record-left" a:if="{{data.isEntry}}">
            <image mode="widthFix" src="https://gw.alipayobjects.com/mdn/industry_l/afts/img/A*gzW2TLL3LawAAAAAAAAAAABkARQnAQ"/>
            入境
        </view>
        <view class="entry-exit-record-left" a:else>
            <image mode="widthFix" src="https://gw.alipayobjects.com/mdn/industry_l/afts/img/A*uHhcTZZuV6QAAAAAAAAAAABkARQnAQ"/>
            出境
        </view>
        <view class="entry-exit-record-right">
            {{data.dateTime}}
        </view>
    </view>
    <view class="entry-exit-record-dashed"></view>
    <view class="entry-exit-record-content">
        <view>
            <view>口岸</view>
            <view>{{data.port}}</view>
        </view>
        <view>
            <view>证件号码</view>
            <view>{{data.credentialNumber}}</view>
        </view>
        <view>
            <view>证件类型</view>
            <view>{{data.credentialType}}</view>
        </view>
        <view>
            <view>交通工具</view>
            <view>{{data.trafficTools}}</view>
        </view>
    </view>
</view>
