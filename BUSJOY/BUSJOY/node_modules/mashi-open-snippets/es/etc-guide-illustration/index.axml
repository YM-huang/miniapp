    <image class="etc-guide-illustration-discounts-icon2" src="https://gw.alipayobjects.com/mdn/industry_l/afts/img/A*vOxXQpI9ec0AAAAAAAAAAABkARQnAQ"></image>
    <image class="etc-guide-illustration-discounts-line" src="https://gw.alipayobjects.com/mdn/industry_l/afts/img/A*7IF8TqnLIdEAAAAAAAAAAABkARQnAQ"></image>
    <image class="etc-guide-illustration-discounts-point" src="https://gw.alipayobjects.com/mdn/industry_l/afts/img/A*C-_DQahzehYAAAAAAAAAAABkARQnAQ"></image>
