.process-wraper .main {
  text-align: left;
  box-sizing: border-box;
  padding: 20rpx 0 68.5rpx 68rpx;
}
.process-wraper .main .item {
  display: inline-block;
}
.process-wraper .main .item .content {
  display: inline-block;
  vertical-align: middle;
  margin-right: 55rpx;
}
.process-wraper .main .item .content .icon {
  width: 85rpx;
  height: 53rpx;
  margin-bottom: 17rpx;
  background-size: cover;
}
.process-wraper .main .item .content .des {
  font-size: 24rpx;
}
.process-wraper .main .item .arrow {
  width: 14rpx;
  height: 22rpx;
  background-size: cover;
  vertical-align: middle;
  margin-right: 74rpx;
}
.process-wraper .particular {
  text-align: left;
  padding: 0 0 22rpx 42rpx;
}
.process-wraper .particular .item .content .icon {
  width: 54rpx;
  height: 42rpx;
  vertical-align: middle;
  margin-right: 45rpx;
}
.process-wraper .particular .item .content .info {
  display: inline-block;
  vertical-align: middle;
}
.process-wraper .particular .item .content .info .title {
  font-size: 30rpx;
  font-weight: bold;
}
.process-wraper .particular .item .content .info .des {
  margin-top: 6rpx;
  font-size: 24rpx;
  color: #666;
}
.process-wraper .particular .item .dashed {
  margin: 7.5rpx 26rpx;
  width: 3rpx;
  height: 69rpx;
  background: url('https://gw.alipayobjects.com/mdn/rms_ec9563/afts/img/A*70DnQKDiWL8AAAAAAAAAAABkARQnAQ') no-repeat;
  background-size: 100% 100%;
}
