 <vtabs
    tabs="{{tabs}}"
    onTabClick="handleChange"
    onChange="onChange"
    activeTab="{{activeTab}}"
    left="{{left}}"
  >
    <block a:for="{{tabs}}" left="{{left}}">
      <vtab-content anchor="{{item.anchor}}" left="{{left}}" style="height:auto;">
        <view class="list-container">
          <view class="list">
              <list className="list-item">
                <view slot="header" class="list-header">{{item.title}}</view>
                <block a:for="{{item.pageList}}">
                    <list-item onClick="onpageItem" data-text="{{item}}" arrow="{{true}}" index="{{index}}" multipleLine="{{false}}" key="items-{{item.title}}" last="{{true}}">
                        <text class="component-name">{{item.title}}</text>
                        <view slot="extra">
                          {{item.extra}}
                        </view>
                    </list-item>
                  </block>
              </list>
          </view>
        </view>
      </vtab-content>
    </block>
  </vtabs>
