.welcome-popup-wraper {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.welcome-popup-wraper > image {
  width: 366rpx;
  height: 320rpx;
}
.welcome-popup-wraper .welcome-popup-title {
  margin-top: 108rpx;
  font-family: PingFangSC-Light;
  font-size: 34rpx;
  color: #FFFFFF;
  letter-spacing: 6.59rpx;
  text-align: center;
}
.welcome-popup-wraper .welcome-popup-subtitle {
  margin-top: 15rpx;
  font-family: PingFangSC-Semibold;
  font-size: 58rpx;
  color: #FFFFFF;
  letter-spacing: 3.65rpx;
}
.welcome-popup-wraper .welcome-popup-subtitle text {
  color: #faaa1d;
}
