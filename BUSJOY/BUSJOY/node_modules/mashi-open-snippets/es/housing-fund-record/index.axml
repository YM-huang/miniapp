<!-- 公积金-历史记录 -->
<view class="housing-fund-record-wraper {{containerClassName}}">
  <view class="housing-fund-record-title">
    <view>{{data.title}}</view>
    <view class="housing-fund-record-in" a:if="{{data.isIn}}">+{{data.value}}</view>
    <view class="housing-fund-record-out" a:else>-{{data.value}}</view>
  </view>
  <view class="housing-fund-record-source-out">
    <view class="housing-fund-record-source">{{data.source}}</view>
    <view class="housing-fund-record-total">{{data.total}}</view>
  </view>
  <view class="housing-fund-record-datetime">{{data.dateTime}}</view>
</view>