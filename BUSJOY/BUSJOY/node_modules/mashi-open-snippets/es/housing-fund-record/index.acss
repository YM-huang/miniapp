.housing-fund-record-wraper {
  display: flex;
  flex-direction: column;
  border-radius: 16rpx;
  background: white;
  min-height: 160rpx;
  box-sizing: border-box;
  padding: 24rpx 24rpx 26rpx 24rpx;
}
.housing-fund-record-wraper .housing-fund-record-title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 16rpx;
}
.housing-fund-record-wraper .housing-fund-record-title > :first-child {
  font-family: PingFangSC-Medium;
  font-size: 30rpx;
  color: #4A4A4A;
  max-width: 400rpx;
}
.housing-fund-record-wraper .housing-fund-record-title .housing-fund-record-in {
  font-family: PingFangSC-Medium;
  font-size: 34rpx;
  color: #FF6010;
  text-align: right;
  line-height: 40rpx;
  max-width: 250rpx;
}
.housing-fund-record-wraper .housing-fund-record-title .housing-fund-record-out {
  font-family: PingFangSC-Medium;
  font-size: 34rpx;
  color: #333333;
  text-align: right;
  line-height: 40rpx;
  max-width: 250rpx;
}
.housing-fund-record-wraper .housing-fund-record-source-out {
  display: flex;
  justify-content: space-between;
  margin-bottom: 16rpx;
}
.housing-fund-record-wraper .housing-fund-record-source-out .housing-fund-record-source {
  font-family: PingFangSC-Regular;
  font-size: 22rpx;
  color: #333333;
}
.housing-fund-record-wraper .housing-fund-record-source-out .housing-fund-record-total {
  font-family: Helvetica;
  font-size: 22rpx;
  color: #999999;
  text-align: right;
}
.housing-fund-record-wraper .housing-fund-record-datetime {
  font-family: Helvetica;
  font-size: 22rpx;
  color: #999999;
  letter-spacing: 0;
}
