.atd-menu-box {
  width: 88rpx;
  border-radius: 44rpx;
  display: flex;
  font-size: 22rpx;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  background: #fff;
  box-shadow: 0 2rpx 10rpx 0 rgba(0, 0, 0, 0.1);
}
.am-popover-placement-rightTop {
  box-shadow: 0 2rpx 10rpx 0 rgba(0, 0, 0, 0.1) !important;
}
.am-popover-placement-rightBottom {
  box-shadow: 0 2rpx 10rpx 0 rgba(0, 0, 0, 0.1) !important;
}
.atd-menu-box .menu-box-commen {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  height: 64rpx;
  padding: 16rpx 0;
  position: relative;
}
.atd-menu-box .popover {
  position: static;
}
.atd-menu-box .popover .item-list {
  height: 400rpx;
  overflow: auto;
}
.atd-menu-box .menu-class-phote {
  width: 30rpx;
  height: 30rpx;
  background: url('https://gw.alipayobjects.com/mdn/rms_7a3c08/afts/img/A*euJORayEK6sAAAAAAAAAAABjARQnAQ') no-repeat;
  background-size: 100%;
}
.atd-menu-box .menu-class-phote:active {
  width: 30rpx;
  height: 30rpx;
  background: url('https://gw.alipayobjects.com/mdn/rms_7a3c08/afts/img/A*-eZQSrYQ3yQAAAAAAAAAAABjARQnAQ') no-repeat;
  background-size: 100%;
}
.atd-menu-box .type-cancel {
  width: 30rpx;
  height: 30rpx;
  background: url('https://gw.alipayobjects.com/mdn/rms_7a3c08/afts/img/A*OTRRRKRFEEcAAAAAAAAAAABkARQnAQ') no-repeat;
  background-size: 100%;
}
