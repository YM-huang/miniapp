.atd-top-tabs {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 150;
  width: 100%;
  display: flex;
  justify-content: space-between;
  background: #fff;
}
.atd-map #map {
  height: 100vh;
  width: 100vw;
}
.atd-list-box {
  position: fixed;
  bottom: 0;
  left: 0;
  height: 600rpx;
  width: 100%;
  z-index: 150;
  background-color: #fff;
}
.atd-left-nav-item {
  height: 60rpx;
  width: 60rpx;
  opacity: 0;
  position: fixed;
  left: 100rpx;
  bottom: 370rpx;
  z-index: 0;
}
.atd-scroll-view-item_H {
  flex-shrink: 0;
  flex-grow: 0;
  width: 200rpx;
}
.atd-scroll-view_H {
  width: 100%;
  white-space: nowrap;
  display: flex;
}
.atd-scroll-view-item-box {
  width: 240rpx;
  margin: 10rpx 30rpx;
  background-color: #f7f7f7;
  padding: 16rpx;
  text-align: center;
}
.atd-scroll-view-item-text {
  font-size: 20rpx;
  color: #b1b1b1;
}
.is-choose {
  background-color: #fff6d5;
}
.atd-top-box {
  height: 68rpx;
  line-height: 68rpx;
  color: skyblue;
  flex: 1;
  text-align: center;
}
.atd-top-click {
  background: #ccc;
}
.atd-menue-item {
  /* height: 80rpx; */
  width: 200rpx;
  padding: 16rpx;
  text-align: center;
}
.atd-tabs-bar-tab {
  height: 42px;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 20%;
  flex-shrink: 0;
  font-size: 16px;
}
.atd-tabs-bar-title {
  height: 42px;
  line-height: 42px;
  box-sizing: border-box;
  position: relative;
  border-bottom-width: 2px;
  border-bottom-style: solid;
  border-bottom-color: transparent;
}
.atd-left-nav {
  position: fixed;
  border-radius: 44rpx;
  bottom: 368rpx;
  left: 25rpx;
  z-index: 150;
  background: #F4F4F4;
}
.atd-left-nav-tp {
  width: 60rpx;
  height: 60rpx;
}
.atd-bottom-sincard {
  position: fixed;
  background: #fff;
  width: 96%;
  bottom: 0;
  left: 2%;
  z-index: 150;
  box-shadow: 0 -1rpx 15rpx 0 rgba(153, 153, 153, 0.4);
  border-radius: 5% 5% 0 0;
}
.atd-icon-tolocation {
  position: fixed;
  width: 88rpx;
  height: 88rpx;
  bottom: 258rpx;
  left: 24rpx;
  z-index: 150;
  /* display:flex;
  justify-content: center;
  align-items: center; */
  border-radius: 50%;
  box-sizing: border-box;
  box-shadow: 0 2rpx 8rpx 0 rgba(0, 0, 0, 0.1);
}
.atd-icon-tolocation.show {
  background: url(https://gw.alipayobjects.com/mdn/rms_7a3c08/afts/img/A*r594Q5HYQgIAAAAAAAAAAABjARQnAQ) #fff no-repeat center center;
  background-size: 60.5%;
}
.atd-icon-tolocation.hidden {
  background: url(https://gw.alipayobjects.com/mdn/rms_7a3c08/afts/img/A*LQm2SoAIwJ0AAAAAAAAAAABjARQnAQ) #fff no-repeat center center;
  background-size: 60.5%;
}
.atd-bottom-sincard-img-box {
  width: 300rpx;
  height: 200rpx;
  margin: 0 auto;
}
.atd-bottom-sincard-img {
  width: 300rpx;
  height: 200rpx;
}
.atd-card-cancel {
  display: flex;
  justify-content: center;
  padding-top: 15rpx;
  padding-bottom: 10rpx;
}
.atd-card-showCardUp {
  width: 73rpx;
  height: 21rpx;
}
.atd-card-showCardUp image {
  display: block;
}
.atd-card-showsv {
  box-sizing: border-box;
  padding: 0 15px;
}
.atd-sc {
  width: 100%;
}
