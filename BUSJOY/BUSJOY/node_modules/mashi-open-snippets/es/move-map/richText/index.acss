  padding-left: 18rpx;
}
.atd-content-right span:nth-of-type(1) {
  text-align: left;
  line-height: 33px;
  font-size: 24px;
  color: #333;
}
.atd-content-rightsp {
  /* overflow:hidden;
  text-overflow:ellipsis; */
  /* color: red;
  white-space: nowrap; */
