<view a:if="{{show}}" class="mapDemo-bottom-card {{open ? 'up' : ''}}">
  <view a:if="{{data.desc}}" class="move" onTap="onMove">
    <image  src="{{!open ? 'https://gw.alipayobjects.com/mdn/rms_7a3c08/afts/img/A*RZF0QLqLOVoAAAAAAAAAAABjARQnAQ':'https://gw.alipayobjects.com/mdn/rms_7a3c08/afts/img/A*V6jPQKkoobIAAAAAAAAAAABjARQnAQ'}}" mode="scaleToFill" />
  </view>
  <view class="close" onTap="onClose">
    <view class="icon">
    </view>
  </view>
  <view class="wrapper">
    <view class="title">
    {{data.title}}
    </view>
    <view class="tag-box">
      <block a:for="{{data.tag}}">
        <text>{{item}}</text>
      </block>
    </view>
    <view class="action">
      <view class="navigation" onTap="onNavigation">
        <image mode="scaleToFill" src="https://gw.alipayobjects.com/mdn/rms_7a3c08/afts/img/A*EUdtSY89FOUAAAAAAAAAAABkARQnAQ"/> 立即前往</view>
    </view>
  </view>
  <view a:if="{{open}}">
    <rich-text html="{{data.desc}}" />
  </view>
</view>