.mapDemo-bottom-card {
  position: fixed;
  background: #fff;
  width: 90%;
  bottom: 2%;
  left: 0;
  right: 0;
  margin-left: auto;
  margin-right: auto;
  z-index: 150;
  box-shadow: 0 -1rpx 15rpx 0 rgba(153, 153, 153, 0.4);
  border-radius: 8rpx;
  padding: 16rpx;
  transition: max-height 0.8s;
  max-height: 300rpx;
}
.mapDemo-bottom-card.up {
  bottom: 0;
  transition: max-height 0.8s;
  max-height: 80vh;
  overflow: auto;
}
.mapDemo-bottom-card .move {
  display: flex;
  justify-content: center;
  padding: 8rpx;
  position: relative;
}
.mapDemo-bottom-card .move image {
  width: 73rpx;
  height: 21rpx;
}
.mapDemo-bottom-card .close {
  position: absolute;
  right: 16rpx;
  top: 16rpx;
}
.mapDemo-bottom-card .close .icon {
  width: 32rpx;
  height: 32rpx;
  border-radius: 24rpx;
  background: url('https://gw.alipayobjects.com/mdn/rms_7a3c08/afts/img/A*mCBsRKW-tTgAAAAAAAAAAABjARQnAQ') no-repeat;
  background-size: 100%;
}
.mapDemo-bottom-card .wrapper {
  padding-left: 16rpx;
}
.mapDemo-bottom-card .title {
  font-size: 42rpx;
  line-height: 60rpx;
  font-weight: bolder;
}
.mapDemo-bottom-card .tag-box {
  padding-top: 16rpx;
  display: flex;
  padding-bottom: 16rpx;
}
.mapDemo-bottom-card .tag-box text {
  height: 36rpx;
  border-radius: 18rpx;
  background: #F2F2F2;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0 12rpx;
  font-size: 24rpx;
  line-height: 24rpx;
  margin-right: 18rpx;
}
.mapDemo-bottom-card .action {
  padding-bottom: 16rpx;
}
.mapDemo-bottom-card .navigation {
  border-radius: 36rpx;
  border: 1rpx solid #ccc;
  height: 72rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  color: #333;
}
.mapDemo-bottom-card .navigation image {
  width: 26rpx;
  height: 24rpx;
  margin-right: 16rpx;
}
