.passway-wraper .out-wraper {
  padding: 0 32rpx;
  display: flex;
  justify-content: space-between;
  text-align: left;
}
.passway-wraper .out-wraper .label {
  font-size: 36rpx;
  line-height: 40rpx;
  vertical-align: middle;
  font-weight: 500;
}
.passway-wraper .out-wraper .more .pre {
  font-size: 24rpx;
  color: #999;
  vertical-align: middle;
}
.passway-wraper .out-wraper .more .arrow {
  width: 12rpx;
  height: 21rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}
.passway-wraper .exit-wraper {
  padding: 40rpx 32rpx 40rpx;
  color: #333;
}
.passway-wraper .exit-wraper .item {
  display: flex;
  justify-content: space-between;
  margin-bottom: 24rpx;
}
.passway-wraper .exit-wraper .item:last-child {
  margin-bottom: 0;
}
.passway-wraper .exit-wraper .item .name {
  font-size: 25rpx;
  line-height: 53rpx;
}
.passway-wraper .exit-wraper .item .list {
  width: 530rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  align-content: flex-start;
}
.passway-wraper .exit-wraper .item .list .exitRef {
  width: 259rpx;
  display: inline-block;
  font-size: 24rpx;
  height: 53rpx;
  line-height: 53rpx;
  background: #F7FCFF;
  border-radius: 2rpx;
  margin-bottom: 10rpx;
}
.passway-wraper .exit-wraper .item .list .exitRef:last-child {
  margin-bottom: 0;
}
.passway-wraper .exit-wraper .item .list .exitRef:nth-last-child(2) {
  margin-bottom: 0;
}
.passway-wraper .traffic-wraper {
  display: flex;
  padding: 32rpx 32rpx 0;
  justify-content: space-between;
  border-top: 1rpx solid #eee;
  font-size: 26rpx;
  font-weight: normal;
}
.passway-wraper .traffic-wraper .label .pre {
  display: inline-block;
  vertical-align: middle;
}
.passway-wraper .traffic-wraper .label .icon {
  display: inline-block;
  height: 34rpx;
  background: rgba(152, 227, 199, 0.24);
  line-height: 34rpx;
  padding: 0 8rpx;
  font-size: 20rpx;
  font-weight: 500;
  color: #11A86E;
  border-radius: 2rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}
.passway-wraper .traffic-wraper .go .text {
  vertical-align: middle;
}
.passway-wraper .traffic-wraper .go .arrow {
  width: 12rpx;
  height: 21rpx;
  margin-left: 10rpx;
  vertical-align: middle;
}
