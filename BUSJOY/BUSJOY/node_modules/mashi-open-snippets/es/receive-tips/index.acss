.main {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 702rpx;
  height: 656rpx;
  background: #ffffff;
  border-radius: 16rpx;
}
.main .main-image {
  margin-top: 62rpx;
  width: 598rpx;
  height: 327rpx;
}
.main .main-text {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #999999;
  letter-spacing: 0.45rpx;
  text-align: center;
  margin-top: 33rpx;
}
.main .main-line {
  margin-top: 42rpx;
  height: 4rpx;
  width: 100%;
  transform: scale(1, 0.5);
  background: #D8D8D8;
}
.main .main-bottom {
  width: 100%;
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}
.main .main-bottom .main-bottom-botton {
  background: #1677FF;
  width: 553rpx;
  height: 90rpx;
  font-size: 36rpx;
  color: #FFFFFF;
  font-family: PingFangSC-Medium;
  text-align: center;
  border-radius: 8px;
  box-shadow: 0 7px 13px 0 rgba(22, 119, 255, 0.23);
}
