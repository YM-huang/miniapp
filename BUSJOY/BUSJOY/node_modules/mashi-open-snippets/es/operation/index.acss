.operation-wraper-item {
  padding: 32rpx 32rpx 34.5rpx;
  border-bottom: 1rpx solid #eee;
  display: flex;
  justify-content: space-between;
}
.operation-wraper-item:first-child {
  padding-top: 0;
}
.operation-wraper-item:last-child {
  border-bottom: none;
  padding-bottom: 0;
}
.operation-wraper-item-label-icon {
  width: 74rpx;
  height: 74rpx;
}
.operation-wraper-item-label-content {
  margin-left: 32rpx;
  display: inline-block;
  line-height: 46rpx;
  text-align: left;
}
.operation-wraper-item-label-content-title {
  font-size: 32rpx;
  color: #3D2E2F;
  font-weight: 500;
}
.operation-wraper-item-label-content-des {
  font-size: 26rpx;
  color: #999;
}
.operation-wraper-item-button {
  width: 124rpx;
  height: 54rpx;
  font-size: 28rpx;
  line-height: 54rpx;
  vertical-align: middle;
  margin-top: 20rpx;
  font-weight: 500;
}
.operation-wraper-item-button.default {
  border-color: #108EE9;
  color: #108EE9;
}
