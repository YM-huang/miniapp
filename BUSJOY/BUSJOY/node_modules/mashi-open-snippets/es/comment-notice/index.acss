.comment-service {
  position: relative;
  display: flex;
  align-items: center;
  background: #FFFFFF;
  box-shadow: 0 2rpx 16rpx 0 rgba(0, 0, 0, 0.05);
  border-radius: 16rpx;
  height: 85rpx;
  margin: 20rpx 32rpx;
}
.comment-service-img {
  width: 41rpx;
  height: 38rpx;
  margin-left: 34rpx;
}
.comment-service-text {
  font-size: 26rpx;
  color: #333;
  margin-left: 25rpx;
}
