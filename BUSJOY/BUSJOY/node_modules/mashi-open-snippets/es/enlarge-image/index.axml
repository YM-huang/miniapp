<view class="atd-enlarge-image">
  <image style="width:{{width?width:'400'}}rpx;height:{{height?height:'400'}}rpx" src="{{url}}" mode="aspectFit" onTap="handelPreview"/>
  <view class="preview-model-{{show ? 'show' : 'hide'}}" disable-scroll="{{true}}">
    <view class="preview-model-mask" onTap="handelClose"></view>
    <view class="preview-model-document">
      <view class="preview-model-content">
        <image class="preview-model-enlarge-image" src="{{url}}" mode="widthFix"/>
        <view class="preview-modal-close" onTap="handelClose">
          <icon type="clear" size="30" color="#dddddd"/>
        </view>
      </view>
    </view>
  </view>
</view>