.traffic-offence-record-wraper {
  display: flex;
  flex-direction: column;
  border-radius: 16rpx;
  overflow: hidden;
  background: white;
  min-height: 350rpx;
  box-sizing: border-box;
}
.traffic-offence-record-wraper .traffic-offence-record-title {
  display: flex;
  justify-content: space-between;
  padding: 20rpx 24rpx;
  background: #f9f9f9;
}
.traffic-offence-record-wraper .traffic-offence-record-title .traffic-offence-record-title-left {
  display: flex;
  align-items: center;
}
.traffic-offence-record-wraper .traffic-offence-record-title .traffic-offence-record-title-left > image {
  width: 44rpx;
  height: 44rpx;
  background-size: contain;
}
.traffic-offence-record-wraper .traffic-offence-record-title .traffic-offence-record-title-left > view {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #333333;
  margin-left: 9.2rpx;
}
.traffic-offence-record-wraper .traffic-offence-record-title .traffic-offence-record-title-right {
  display: flex;
  align-items: center;
}
.traffic-offence-record-wraper .traffic-offence-record-title .traffic-offence-record-title-right > :first-child {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #999999;
  margin-right: 22.5rpx;
}
.traffic-offence-record-wraper .traffic-offence-record-title .traffic-offence-record-title-right > :last-child {
  width: 16.4rpx;
  height: 26rpx;
  background-image: url('https://gw.alipayobjects.com/zos/rmsportal/nGMpTwTjtKMbOeeQIucS.png');
  background-size: contain;
  background-repeat: no-repeat;
  background-position: 50% 50%;
}
.traffic-offence-record-wraper .traffic-offence-record-content {
  display: flex;
  flex-direction: column;
  padding: 32rpx 24rpx;
}
.traffic-offence-record-wraper .traffic-offence-record-content .traffic-offence-record-value {
  display: flex;
  align-items: center;
  margin-bottom: 8rpx;
}
.traffic-offence-record-wraper .traffic-offence-record-content .traffic-offence-record-value :first-child {
  font-family: PingFangSC-Regular;
  font-size: 36rpx;
  color: #333333;
}
.traffic-offence-record-wraper .traffic-offence-record-content .traffic-offence-record-value :last-child {
  font-family: Helvetica;
  font-size: 60rpx;
  color: #333333;
  max-width: 600rpx;
  word-wrap: break-word;
}
.traffic-offence-record-wraper .traffic-offence-record-content .traffic-offence-record-row {
  display: flex;
  margin-bottom: 10rpx;
}
.traffic-offence-record-wraper .traffic-offence-record-content .traffic-offence-record-row :first-child {
  min-width: 120rpx;
  margin-right: 16rpx;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #999999;
}
.traffic-offence-record-wraper .traffic-offence-record-content .traffic-offence-record-row :last-child {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #333333;
}
.traffic-offence-record-wraper .traffic-offence-record-content :last-child {
  margin: 0;
}
