.etc-confirm-wraper {
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  width: 100%;
  height: 100vh;
  top: 0;
  left: 0;
  z-index: 2000;
  background: rgba(0, 0, 0, 0.7);
}
.etc-confirm-wraper .etc-confirm-content {
  position: relative;
  width: 540rpx;
  height: 607rpx;
  background: #FFFFFF;
  border-radius: 16rpx;
  background-image: url(https://gw.alipayobjects.com/mdn/industry_l/afts/img/A*QU4gQqKWBicAAAAAAAAAAABkARQnAQ);
  background-size: 540rpx 300rpx;
  background-repeat: no-repeat;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}
.etc-confirm-wraper .etc-confirm-close {
  position: absolute;
  top: 28rpx;
  right: 32rpx;
  width: 30rpx;
  height: 30rpx;
  background-image: url(https://gw.alipayobjects.com/mdn/industry_l/afts/img/A*vFDHS5ywXE0AAAAAAAAAAABkARQnAQ);
  background-size: 100%;
}
.etc-confirm-wraper .etc-confirm-title {
  margin-top: 340rpx;
  font-family: PingFangSC-Medium;
  font-size: 36rpx;
  color: #333333;
  letter-spacing: 0.58rpx;
  text-align: center;
  line-height: 42rpx;
}
.etc-confirm-wraper .etc-confirm-subtitle {
  margin-top: 20rpx;
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  color: #FF6010;
  letter-spacing: 0.58rpx;
  text-align: center;
  line-height: 40rpx;
}
.etc-confirm-wraper .etc-confirm-buttons {
  position: absolute;
  bottom: 0;
  display: flex;
}
.etc-confirm-wraper .etc-confirm-buttons button {
  width: 270rpx;
}
.etc-confirm-wraper .etc-confirm-buttons :first-child {
  font-family: PingFangSC-Regular;
  font-size: 36rpx;
  color: #999999;
  letter-spacing: 0.58rpx;
  text-align: center;
  border-right: none;
}
.etc-confirm-wraper .etc-confirm-buttons :last-child {
  font-family: PingFangSC-Medium;
  font-size: 36rpx;
  color: #1677FF;
  letter-spacing: 0.58rpx;
}
