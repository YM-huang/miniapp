<!-- etc-交易记录 -->
<view class="transaction-record-wraper {{containerClassName}}">
    <view class="transaction-record-top">
        <view class="transaction-record-title">{{data.title}}</view>
        <view class="transaction-record-value">{{data.value}}</view>
    </view>
    <view class="transaction-record-content">
        <view>
            <view class="transaction-record-start">
                <view class="transaction-record-icon" />
                {{data.inAddress}}
            </view>
            <view class="transaction-record-end">
                <view class="transaction-record-icon" />
                {{data.outAddress}}
            </view>
        </view>
        <view style="color:{{data.statusColor?data.statusColor:defaultStatusColor}}">
            {{data.status}}
        </view>
    </view>
</view>