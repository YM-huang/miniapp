.transaction-record-wraper {
  display: flex;
  flex-direction: column;
  border-radius: 16rpx;
  background: white;
  min-height: 223rpx;
  padding: 27rpx 24rpx 32rpx 24rpx;
  box-sizing: border-box;
}
.transaction-record-wraper .transaction-record-top {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 21rpx;
}
.transaction-record-wraper .transaction-record-top .transaction-record-title {
  max-width: 500rpx;
  font-family: Helvetica;
  font-size: 26rpx;
  color: #333333;
}
.transaction-record-wraper .transaction-record-top .transaction-record-value {
  font-family: Helvetica;
  font-size: 36rpx;
  color: #333333;
  line-height: 48rpx;
  max-width: 170rpx;
  text-align: right;
}
.transaction-record-wraper .transaction-record-content {
  display: flex;
  justify-content: space-between;
}
.transaction-record-wraper .transaction-record-content > :first-child {
  display: flex;
  flex-direction: column;
}
.transaction-record-wraper .transaction-record-content > :first-child .transaction-record-start {
  display: flex;
  align-items: center;
  max-width: 500rpx;
  margin-bottom: 12rpx;
}
.transaction-record-wraper .transaction-record-content > :first-child .transaction-record-start .transaction-record-icon {
  width: 13rpx;
  height: 13rpx;
  border-radius: 50%;
  border: 2px solid #1777FF;
  margin-right: 16rpx;
}
.transaction-record-wraper .transaction-record-content > :first-child .transaction-record-end {
  display: flex;
  align-items: center;
  max-width: 500rpx;
}
.transaction-record-wraper .transaction-record-content > :first-child .transaction-record-end .transaction-record-icon {
  width: 13rpx;
  height: 13rpx;
  border-radius: 50%;
  border: 2px solid #FF6010;
  margin-right: 16rpx;
}
.transaction-record-wraper .transaction-record-content > :last-child {
  font-family: PingFangSC-Medium;
  font-size: 24rpx;
  color: #999999;
  letter-spacing: 0.39rpx;
  text-align: right;
}
