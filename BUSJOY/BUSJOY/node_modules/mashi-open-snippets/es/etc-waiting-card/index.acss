.etc-waiting-card-wraper {
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 445rpx;
  box-sizing: border-box;
  background: white;
  padding-top: 64rpx;
}
.etc-waiting-card-wraper .etc-waiting-card-title {
  font-family: PingFangSC-Medium;
  font-size: 54rpx;
  color: #333333;
  margin-bottom: 38rpx;
}
.etc-waiting-card-wraper .etc-waiting-card-subtitle-out {
  height: 106rpx;
  margin: 0 30rpx;
}
.etc-waiting-card-wraper .etc-waiting-card-subtitle-out .etc-waiting-card-subtitle {
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #333333;
}
.etc-waiting-card-wraper .etc-waiting-card-info-out {
  display: flex;
  justify-content: center;
  align-items: center;
}
.etc-waiting-card-wraper .etc-waiting-card-info-out image {
  width: 40rpx;
  height: 67rpx;
}
.etc-waiting-card-wraper .etc-waiting-card-info-out view {
  max-width: 600rpx;
  background: rgba(255, 230, 216, 0.3);
  border-radius: 28rpx;
  display: flex;
  align-items: center;
  padding: 10rpx 24rpx;
}
.etc-waiting-card-wraper .etc-waiting-card-info-out view text {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #FF6010;
  text-align: center;
}
