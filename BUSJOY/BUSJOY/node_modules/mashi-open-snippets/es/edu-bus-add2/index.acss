.main {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 702rpx;
  height: 614rpx;
  background: #ffffff;
  border-radius: 16rpx;
  padding-bottom: 42rpx;
}
.main .main-image {
  margin-top: 62rpx;
  width: 608rpx;
  height: 338rpx;
}
.main .main-text {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #999999;
  letter-spacing: 0.45rpx;
  text-align: center;
  margin-top: 20rpx;
}
.main .main-line {
  width: 100%;
  height: 1px;
  transform: scaleY(0.35);
  background: #d8d8d8;
  margin-top: 42rpx;
}
.main .main-bottom {
  width: 100%;
  display: flex;
  justify-content: center;
}
.main .main-bottom .main-bottom-botton {
  background: #1677ff;
  width: 553rpx;
  height: 90rpx;
  margin-top: 32rpx;
  font-size: 36rpx;
  color: #ffffff;
  font-family: PingFangSC-Medium;
  text-align: center;
  border-radius: 8px;
  box-shadow: 0 7px 13px 0 rgba(22, 119, 255, 0.23);
}
